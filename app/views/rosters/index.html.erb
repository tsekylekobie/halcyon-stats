<p id="notice"><%= notice %></p>

<h1>Rosters</h1>

<table>
  <thead>
    <tr>
      <th>Id</th>
      <th>Won</th>
      <th>Left</th>
      <th>Score</th>
      <th>Kraken</th>
      <th>Aces</th>
      <th>Turretskilled</th>
      <th>Turretsremaining</th>
      <th>Gold</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @rosters.each do |roster| %>
      <tr>
        <td><%= roster.id %></td>
        <td><%= roster.won %></td>
        <td><%= roster.left %></td>
        <td><%= roster.score %></td>
        <td><%= roster.kraken %></td>
        <td><%= roster.aces %></td>
        <td><%= roster.turretsKilled %></td>
        <td><%= roster.turretsRemaining %></td>
        <td><%= roster.gold %></td>
        <td><%= link_to 'Show', roster %></td>
        <td><%= link_to 'Edit', edit_roster_path(roster) %></td>
        <td><%= link_to 'Destroy', roster, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Roster', new_roster_path %>
